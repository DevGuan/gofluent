{
	"sources": [
		{
			"type": "tail",
			"path": "/usr/local/nginx/logs/error.log",
			"format": "(?P<msg>.*)",
			"tag": "debug.test"
		},
		{
			"type": "tail",
			"path": "/usr/local/nginx/logs/access.log",
			"format": "(?P<test>.*)",
			"tag": "nginx.access"
		}
	],

	"matches": [
		{
			"tag": "debug.*",
			"type": "forward",
			"host": "172.19.108.101",
			"port": 8888,
			"heartbeat_interval": 1
		},
		{
			"tag": "nginx.access",
			"type": "stdout"
		}
	]
}
